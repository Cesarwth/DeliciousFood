{
  "databaseChangeLog": [
    {
      "changeSet": {
        "id": "create-rooms-table",
        "author": "All",
        "changes": [
          {
            "createTable": {
              "tableName": "rooms",
              "columns": [
                { "column": { "name": "id", "type": "INTEGER", "autoIncrement": "true",
                  "constraints": { "nullable": false, "primaryKey": true } } },
                { "column": { "name": "room_description", "type": "varchar(4000)", "constraints": { "nullable": false } } },
                { "column": { "name": "price", "type": "DECIMAL", "constraints": { "nullable": false } } },
                { "column": { "name": "max_stay_days", "type": "integer", "constraints": { "nullable": false } } },
                { "column": { "name": "status", "type": "varchar(20)", "constraints": { "nullable": false } } }
              ]
            }
          }
        ]
      }
    }
  ]
}
