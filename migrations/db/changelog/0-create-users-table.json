{
  "databaseChangeLog": [
    {
      "changeSet": {
        "id": "create-users-table",
        "author": "all",
        "changes": [
          {
            "createTable": {
              "tableName": "users",
              "columns": [
                { "column": { "name": "id", "type": "INTEGER", "autoIncrement": "true",
                  "constraints": { "nullable": false, "primaryKey": true } } },
                { "column": { "name": "name", "type": "varchar(300)", "constraints": { "nullable": false } } },
                { "column": { "name": "phone", "type": "varchar(15)", "constraints": { "nullable": false } } },
                { "column": { "name": "email", "type": "varchar(300)", "constraints": { "nullable": false } } },
                { "column": { "name": "country", "type": "varchar(50)", "constraints": { "nullable": false } } }
              ]
            }
          }
        ]
      }
    }
  ]
}
